			<% count = 0 %>
			<% if @artists %>
				<% @artists.each do |val| %>
					<% if val.name_id == 'reception' || val.multi_location == true %>
					<% else %>
					  <div class="panel panel-default" id="scroll<%= val.name_id %>">
					    <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= count %>" aria-expanded="false" aria-controls="collapseOne">
						    <div class="panel-heading" role="tab" id="headingOne">
						      <h4 class="panel-title"><%= val.name %> <span class="title-ital">- <%= val.title %></span></h4>
						    </div>
						</a>
					    <div id="collapse<%= count %>" class="scroll<%= val.name_id %> panel-collapse collapse<% if count == 0 %> in <% end %>" role="tabpanel" aria-labelledby="headingOne">
					      <div class="panel-body">
					      	<h5 class="wherewhen"><%= val.where %> - <%= val.when %></h5>
					      	<% if val.avatar.file? %>
						      	<%= image_tag val.avatar.url(:large), :class => "img-fluid" %>
					      	<% end %>
					        <%= simple_format(val.copy) %>
					        <%= simple_format(val.bio) %>
					      </div>
					    </div>
					  </div>
					  <% end %>
					<% count += 1 %>
				  <% end %>
			  <% end %>
			</div>

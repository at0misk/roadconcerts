  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<div class="container">
	<div class="col-sm-6">
		<form method="post" action="/artists">
		    <input type='hidden' name='authenticity_token' value='<%= form_authenticity_token %>'>
			<div class="col-sm-12">Name: <input type="text" name="artist[name]"></div>
			<div class="col-sm-12">Title: <input type="text" name="artist[title]"></div>
			<div class="col-sm-12">Copy (short): <textarea name="artist[copy]" rows="10" cols="40"></textarea></div>
			<div class="col-sm-12">Bio / Description: <textarea name="artist[bio]" rows="10" cols="40"></textarea></div>
			<div class="col-sm-12">where: <input type="text" name="artist[where]"></div>
			<div class="col-sm-12">name_id: <input type="text" name="artist[name_id]"></div>
			<div class="col-sm-12">when: <input type="text" name="artist[when]"></div>
			<div class="col-sm-12">lat: <input type="text" name="artist[lat]"></div>
			<div class="col-sm-12">long: <input type="text" name="artist[long]"></div>
			<div class="col-sm-12">facebook: <input type="text" name="artist[facebook]"></div>
			<div class="col-sm-12">twitter: <input type="text" name="artist[twitter]"></div>
			<div class="col-sm-12">insta: <input type="text" name="artist[insta]"></div>
			<div class="col-sm-12">website: <input type="text" name="artist[website]"></div>
			<div class="col-sm-12">downloadable: <input type="checkbox" name="artist[downloadable]"></div>
			<div class="col-sm-12">multi location: <input type="checkbox" name="artist[multi_location]"></div>
			<input type="submit" value="GO">
		</form>
	</div>
</div>